{
  "name": "ng2-f-template-spa",
  "version": "0.0.1",
  "description": "Seed template for SPA",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/fragaria/ng2-f-template-spa.git"
  },
  "scripts": {
    "gulp": "gulp",
    "preinit": "npm version 0.0.0-SNAPSHOT --no-git-tag-version",
    "init": "gulp init",
    "init-conf": "gulp init-conf",
    "bump": "gulp bump",
    "sonar": "gulp sonar",
    "jenkins": "gulp jenkins",
    "preprotractor": "webdriver-manager update",
    "protractor": "gulp protractor",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack -d --config config/webpack.develop.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:test": "npm run clean:dist",
    "build:test": "webpack --config config/webpack.test.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack -p --config config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "e2e": "npm-run-all -p -r start protractor",
    "lint": "npm run lint:ts",
    "lint:ts": "tslint src/app/{,**/}*.ts",
    "lint:sass": "sass-lint",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server -d --config config/webpack.develop.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "preserver:prod": "npm run build:prod",
    "server:prod": "http-server dist --cors",
    "start": "npm run server",
    "pretest": "npm run lint",
    "test": "karma start",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' --exclude '**/index.ts' ./src/"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.51",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "0.0.28",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-exists": "^2.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.26.7",
    "gulp": "^3.9.1",
    "gulp-bump": "^0.3.1",
    "gulp-git": "^1.7.2",
    "gulp-jenkins": "ondrejchmelar/gulp-jenkins.git#v0.1.2",
    "gulp-maven-deploy": "^0.2.0",
    "gulp-pipe": "^1.0.4",
    "gulp-plumber": "^1.0.1",
    "gulp-prompt": "^0.2.0",
    "gulp-protractor": "^3.0.0",
    "gulp-replace": "^0.5.4",
    "gulp-shell": "^0.5.2",
    "gulp-sonar": "^2.1.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-util": "^3.0.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.0.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "^0.1.0",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "~3.7.0",
    "npm-run-all": "^3.1.1",
    "null-loader": "^0.1.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss": "^5.2.4",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.8",
    "remap-istanbul": "^0.6.4",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.2",
    "sass-lint": "^1.7.0",
    "sass-loader": "^3.2.0",
    "sonar-web-frontend-duplication": "^1.0.1",
    "sonar-web-frontend-reporters": "^2.1.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "1.1.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.4",
    "typedoc": "^0.4.5",
    "typescript": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-combine-loaders": "^2.0.0",
    "webpack-dev-server": "^1.15.2",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.1",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.1",
    "@angular/http": "^2.0.1",
    "@angular/platform-browser": "^2.0.1",
    "@angular/platform-browser-dynamic": "^2.0.1",
    "@angular/platform-server": "^2.0.1",
    "@angular/router": "^3.0.1",
    "angular2-toaster": "1.0.1",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "ng2-f-mock-api": "~0.0.11",
    "ng2-translate": "^4.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  }
}
